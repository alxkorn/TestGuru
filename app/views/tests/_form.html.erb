<h1><%= test_header(@test) %></h1>
<%= render 'shared/error', resource: @test %>
<%= form_with model: @test, local: true do |form| %>
  <p>  
    <%= form.label :title %>
    <%= form.text_field :title%>
  </p>
  <p>  
    <%= form.label :level %>
    <%= form.number_field :level%>
  </p>
  <p>
    <%= form.label :category_id %>
    <%= form.collection_select :category_id, Category.all, :id, :title, prompt: true %>
  </p>
  <p>
    <%= form.label :author_id %>
    <%= form.collection_select :user_id, User.authors, :id, :name, prompt: true %>
  </p>
  <%= form.submit %>
<% end %>
